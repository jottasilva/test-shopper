datasource db {
  provider = "sqlite"
  url      =  env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Measureuser {
  id                Int      @id @default(autoincrement())
  customer_code     String   @unique
  measure_value     String   @unique
  measure_type      String?
  measure_datetime  DateTime @default(now())
}
